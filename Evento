//Clase Hijo - Evento
import java.util.Date;
import java.util.ArrayList;

public class evento extends Agenda{
    protected Date fechaFin;
    ArrayList<String> invitados;

    Evento(int id, String nombre, String descripcion, String lugar, ArrayList invitados, Date fechaInicio, Date fechaFin){
        super(id, nombre, descripcion, lugar, fechaInicio);
        this.fechaFin = fechaFin;
        this.invitados = invitados;
    }

    public Date getFechaFin(){
        return this.fechaFin;
    }
}